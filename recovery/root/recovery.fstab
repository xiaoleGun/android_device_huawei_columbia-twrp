/system_root       ext4      /dev/block/bootdevice/by-name/system            flags=display="System";backup=1
/kernel            emmc      /dev/block/bootdevice/by-name/kernel            flags=display="Kernel";backup=1;flashimg=1
/recovery_ramdisk  emmc      /dev/block/bootdevice/by-name/recovery_ramdisk  flags=display="Recovery Ramdisk";backup=1;flashimg=1
/cache             ext4      /dev/block/bootdevice/by-name/cache             flags=display="Cache"
/cust              ext4      /dev/block/bootdevice/by-name/cust              flags=display="Cust";backup=1,flashimg=1
/misc              emmc      /dev/block/bootdevice/by-name/misc              flags=display="Misc";backup=0
/oeminfo           emmc      /dev/block/bootdevice/by-name/oeminfo           flags=display="OEMinfo";backup=1
/data              f2fs      /dev/block/bootdevice/by-name/userdata
/system_image      emmc      /dev/block/bootdevice/by-name/system            flags=display="System Image";backup=1
/vendor            ext4      /dev/block/bootdevice/by-name/vendor
/vendor_image      emmc      /dev/block/bootdevice/by-name/vendor            flags=display="Vendor Image";backup=1
/odm               ext4      /dev/block/bootdevice/by-name/odm               flags=flashimg=1;backup=1;display="ODM";flashimg=1
/splash2	       ext4		 /dev/block/bootdevice/by-name/splash2		     flags=flashimg=1;backup=1;display="Splash2"
/hisee_fs	       ext4		 /dev/block/bootdevice/by-name/hisee_fs			 flags=backup=1;display="Hisee FS"
/product	       ext4		 /dev/block/bootdevice/by-name/product			 flags=display="Product";backup=1;wipeingui
/version	       ext4		 /dev/block/bootdevice/by-name/version			 flags=display="Version";backup=1;wipeingui

#Removable Storage#
/usbotg		       auto		 /dev/block/sde1         /dev/block/sde          flags=display="USB OTG";storage;removable;backup=0
